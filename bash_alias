# Shortcuts
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias vi=vim
alias python=python3

# Credit to Strager https://github.com/strager/dotfiles
# Switch repos
alias cd.='cd ../'
alias cd..='cd ../../'
alias cd...='cd ../../../'
alias h='cd ~/'

# Git
alias gs='git status'
alias gc='git commit -m'
alias g.='git add .'
alias g?='git diff'
alias g^='git push'
