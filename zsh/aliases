# Credit to Strager https://github.com/strager/dotfiles/

# Git
alias gs="git status -s -b"
alias gc="git commit -v"
alias g.="git add -p"
alias gi="git add -i"
alias g^="git push"
alias gd="git diff"

# Shortcut
alias cd.="cd ../"
alias cd..="cd ../../"
alias cd...="cd ../../../"
alias vi=vim
alias python=python3
alias pip=pip3
