# Shortcuts
alias vi=vim
alias python=python3

# Credit to Strager https://github.com/strager/dotfiles
# Switch repos
alias cd.='cd ../'
alias cd..='cd ../../'
alias cd...='cd ../../../'

# Git
alias gs='git status -s -b'
alias gc='git commit -v'
alias g.='git add -p'
alias g?='git diff'
alias g^='git push'
